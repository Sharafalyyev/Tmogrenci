<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TmogrenciButton" parent="Widget.Material3.Button">
        <item name="android:fontFamily">@font/urbanist_bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/primary_green</item>
        <item name="cornerRadius">16dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
    </style>
    <style name="TmogrenciHeader" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:fontFamily">@font/urbanist_bold</item>
        <item name="android:textColor">@color/primary_green</item>
        <item name="android:textSize">28sp</item>
    </style>
    <style name="ShapeAppearance.MaterialComponents.CircularImageView" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources> 